{
  "$schema": "https://schemas.wp.org/trunk/block.json",
  "apiVersion": 2,
  "name": "create-block/single-movie",
  "version": "0.1.0",
  "title": "Single Movie",
  "category": "widgets",
  "icon": "format-video",
  "description": "A Gutenberg block to display a single movie using search feature which gives you autocomplete suggestions of movies.",
  "attributes": {
    "movieSearch": {
      "type": "string",
      "default": ""
    },
    "movieId": {
      "type": "number",
      "default": 0
    }
  },
  "supports": {
    "html": false,
    "spacing": {
      "margin": true,
      "padding": true
    },
    "color": {
      "background": true,
      "text": true
    }
  },
  "textdomain": "movie-library",
  "editorScript": "file:./index.js",
  "editorStyle": "file:./index.css",
  "style": "file:./style-index.css"
}