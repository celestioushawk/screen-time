{
  "$schema": "https://schemas.wp.org/trunk/block.json",
  "apiVersion": 2,
  "name": "create-block/person-list",
  "version": "0.1.0",
  "title": "Person List",
  "category": "text",
  "icon": "businessman",
  "description": "A Gutenberg block that shows list of people based on filters such as person career and count.",
  "attributes": {
    "personCareer": {
      "type": "string",
      "default": ""
    },
    "personCount": {
      "type": "number",
      "default": 5
    }
  },
  "supports": {
    "html": false,
    "spacing": {
      "margin": true,
      "padding": true
    },
    "color": {
      "background": true,
      "text": true
    }
  },
  "textdomain": "movie-library",
  "editorScript": "file:./index.js",
  "editorStyle": "file:./index.css",
  "style": "file:./style-index.css"
}